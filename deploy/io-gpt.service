[Unit]
Description=hypercorn instance to serve io-gpt
After=network.target

[Service]
User=namin
Group=www-data
WorkingDirectory=/home/namin/io-gpt.livecode.ch
Environment="PATH=/home/namin/io-gpt.livecode.ch/io-gpt-env/bin"
ExecStart=/home/namin/io-gpt.livecode.ch/io-gpt-env/bin/hypercorn -b unix:io-gpt.sock -w 3 -g 33 -m 775 main:app

[Install]
WantedBy=multi-user.target
